<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"

            fx:controller="com.example.project11.Controllers.Charts.BarChartController"
            >

    <children>

        <BarChart fx:id="barChart" prefHeight="250.0" prefWidth="380.0">
            <xAxis>
                <CategoryAxis fx:id="xAxis" label="Grade" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" label="Frequency" />
            </yAxis>
        </BarChart>
    </children>

</AnchorPane>
